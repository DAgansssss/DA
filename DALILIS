rotatingimage[5]="https://scontent-nrt1-1.xx.fbcdn.net/hphotos-xpf1/v/t1.0-9/12033223_969300546469924_6074088804013546935_n.jpg?oh=d48f02de43d82e0939dfab59319c7bdc&oe=56D1B64D"


// Set the the comments corresponding to the images above (requires the same amount as images!)
